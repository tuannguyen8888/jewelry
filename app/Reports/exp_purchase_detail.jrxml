		<parameter name="user_ids" class="java.lang.String"/>
		<parameter name="from_date" class="java.util.Date"/>
		<parameter name="to_date" class="java.util.Date"/>
		<queryString language="SQL">
			<![CDATA[SELECT name, SUM(qty) as qty, SUM(gold_weight) AS gold_weight, age, SUM(q10) AS q10, SUM(amount) AS amount, SUM(fee) AS fee
FROM(
	SELECT P.name, COUNT(POD.id) as qty, SUM(POD.gold_weight) AS gold_weight, POD.age, SUM(POD.q10) AS q10, 
		SUM(POD.amount) AS amount, SUM(-(PO.fee * (POD.amount / PO.amount))) AS fee
	FROM gold_purchase_orders PO
		INNER JOIN gold_counters C ON PO.counter_id = C.id
		INNER JOIN gold_purchase_order_details POD ON PO.id = POD.order_id
		LEFT OUTER JOIN gold_product_types P ON POD.product_type_id = P.id	
	WHERE PO.deleted_at IS NULL AND POD.deleted_at IS NULL AND FIND_IN_SET(C.saler_id, $P{user_ids}) > 0
		AND PO.order_date >= $P{from_date} AND PO.order_date < DATE_ADD($P{to_date}, INTERVAL 1 DAY)
	GROUP BY P.name, POD.age
	UNION ALL
	SELECT P.name, COUNT(POD.id) as qty, SUM(POD.gold_weight) AS gold_weight, POD.age, SUM(POD.q10) AS q10, 
		SUM(POD.amount) AS amount, SUM(POD.fee) AS fee
	FROM gold_sale_orders PO
		INNER JOIN gold_counters C ON PO.counter_id = C.id
		INNER JOIN gold_sale_order_pays POD ON PO.id = POD.order_id
		LEFT OUTER JOIN gold_product_types P ON POD.product_type_id = P.id	
	WHERE PO.deleted_at IS NULL AND POD.deleted_at IS NULL AND FIND_IN_SET(C.saler_id, $P{user_ids}) > 0
		AND PO.order_date >= $P{from_date} AND PO.order_date < DATE_ADD($P{to_date}, INTERVAL 1 DAY)
	GROUP BY P.name, POD.age
) A
GROUP BY name, age
ORDER BY name, age]]>
		</queryString>

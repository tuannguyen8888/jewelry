	<queryString language="SQL">
		<![CDATA[SELECT B.name AS brand_name, U.employee_code, U.name AS emp_name, A.type, A.order_date, A.order_no, A.notes, A.age
	,SUM(A.gold_weight) AS gold_weight, ROUND(SUM(A.q10), 4) as q10, ROUND(SUM(A.amount), 0) as amount
FROM(
	SELECT '' AS type, brand_id, saler_id, NULL AS order_date, '' AS order_no, NULL AS notes, 0 AS age
		,0 AS gold_weight, ROUND(SUM(q10), 4) AS q10, ROUND(SUM(amount), 0) AS amount
	FROM v_user_trans
	WHERE order_date < $P{from_date}
		AND FIND_IN_SET(saler_id, $P{ids}) > 0
		AND brand_id = $P{brand_id}
	GROUP BY brand_id, saler_id
	HAVING ROUND(SUM(q10), 4) <> 0 OR ROUND(SUM(amount), 0) <> 0
	UNION ALL
	SELECT type, brand_id, saler_id, order_date, order_no, notes, age, gold_weight, q10, amount
	FROM v_user_trans
	WHERE order_date BETWEEN $P{from_date} AND DATE_ADD($P{to_date}, INTERVAL 1 DAY)
		AND FIND_IN_SET(saler_id, $P{ids}) > 0
		AND brand_id = $P{brand_id}
) A INNER JOIN cms_users U ON A.saler_id = U.id
	INNER JOIN gold_brands B ON A.brand_id = B.id
GROUP BY B.name, U.employee_code, U.name, A.order_date, A.order_no, A.notes, A.age
HAVING ROUND(SUM(A.q10), 4) <> 0 OR ROUND(SUM(A.amount), 0) <> 0
ORDER BY B.name, U.employee_code, A.type, A.order_date, A.order_no]]>
	</queryString>

	<queryString language="SQL">
		<![CDATA[SELECT SO.order_no, SO.order_date, COUNT(SOD.id) AS No, C.name AS customer_name, C.address, C.phone, C.zalo_phone, C.tax_no,
	SUM(SOD.edit_weight + I.gem_weight) AS total_weight, SUM(I.gem_weight) AS gem_weight, SUM(SOD.edit_weight) AS edit_weight, 
	SUM(SOD.fee) AS fee, SUM(SOD.discount) AS discount, SUM(SOD.amount) AS amount,
	SO.reduce, SO.pay_gold_amount, SO.return_amount, SO.points, SO.payment_method, SO.balance, SO.use_points,
	ROUND(SO.pay_amount - SO.return_amount) AS pay_amount, ROUND((SO.pay_amount - SO.return_amount) / (1 + SO.bank_fee / 100) * SO.bank_fee / 100) AS bank_amount,
	(SELECT COUNT(id) FROM gold_sale_order_pays WHERE deleted_at IS NULL AND order_id = SO.id) AS pays, SO.bank_fee, U.name AS user_name, B.name AS brand_name
FROM gold_sale_orders SO
	LEFT OUTER JOIN gold_sale_order_details SOD ON SO.id = SOD.order_id
	LEFT OUTER JOIN gold_items I ON SOD.item_id = I.id
	LEFT OUTER JOIN gold_customers C ON SO.customer_id = C.id
	LEFT OUTER JOIN cms_users U ON SO.saler_id = U.id
	LEFT OUTER JOIN gold_brands B ON SO.brand_id = B.id
WHERE SO.id =  $P{id} 
GROUP BY SO.order_no, SO.order_date, SO.reduce, C.name, C.address, C.zalo_phone, C.tax_no, SO.bank_fee,
	SO.pay_amount, SO.pay_gold_amount, SO.return_amount, SO.points, SO.payment_method, SO.balance, SO.use_points, U.name, B.name]]>
	</queryString>

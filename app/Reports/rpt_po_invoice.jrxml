		<parameter name="order_id" class="java.lang.Integer"/>
		<queryString language="SQL">
			<![CDATA[SELECT SOP.description, PT.name AS product_type,
	SOP.gem_weight, SOP.gold_weight, SOP.price, SOP.amount,
	(SOP.total_weight - SOP.abate_weight) AS total_weight
FROM gold_purchase_order_details SOP
	LEFT OUTER JOIN gold_product_types PT ON SOP.product_type_id = PT.id
WHERE SOP.deleted_at IS NULL
	AND SOP.order_id =  $P{order_id}]]>
		</queryString>

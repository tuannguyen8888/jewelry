		<queryString language="SQL">
			<![CDATA[SELECT NULL AS order_no, NULL as order_date, N'Số dư đầu kỳ' AS notes, NULL as object
	,SUM(in_amount) as in_amount, SUM(out_amount) as out_amount 
FROM gold_vouchers
WHERE deleted_at IS NULL AND status <> 0 	AND is_balance = 0 AND order_type IN(0, 1)
	AND order_date < $P{from_date} AND brand_id = $P{brand_id} 
HAVING SUM(in_amount) - SUM(out_amount) <> 0
UNION ALL
SELECT V.order_no, V.order_date, V.notes, IFNULL(C.name, IFNULL(S.name, IFNULL(I.name, U.name))) as object
	,V.in_amount as in_amount, V.out_amount 
FROM gold_vouchers V
	LEFT OUTER JOIN gold_customers C ON V.object_id = C.id AND V.object_type = 0
	LEFT OUTER JOIN gold_suppliers S ON V.object_id = S.id AND V.object_type = 1
	LEFT OUTER JOIN gold_investors I ON V.object_id = I.id AND V.object_type = 2
	LEFT OUTER JOIN cms_users U ON V.object_id = U.id AND V.object_type = 3
WHERE V.deleted_at IS NULL AND V.status <> 0 AND V.is_balance = 0 AND V.order_type IN(0, 1)
	AND V.order_date >= $P{from_date} AND V.order_date < DATE_ADD($P{to_date}, INTERVAL 1 DAY)
	AND V.brand_id = $P{brand_id}
ORDER BY order_date, order_no]]>
		</queryString>

	<queryString language="SQL">
		<![CDATA[SELECT SO.order_no, SO.order_date, COUNT(SOD.id) AS No, C.name AS customer_name, C.address, C.zalo_phone, C.tax_no,
	SUM(SOD.total_weight - SOD.abate_weight) AS total_weight, SUM(SOD.gem_weight) AS gem_weight, SUM(SOD.gold_weight) AS gold_weight, 
	SO.fee, SO.amount
FROM gold_purchase_orders SO
	LEFT OUTER JOIN gold_purchase_order_details SOD ON SO.id = SOD.order_id
	LEFT OUTER JOIN gold_customers C ON SO.customer_id = C.id
WHERE SO.id =  $P{id} 
GROUP BY SO.order_no, SO.order_date, SO.fee, SO.amount,
	C.name, C.address, C.zalo_phone, C.tax_no]]>
	</queryString>

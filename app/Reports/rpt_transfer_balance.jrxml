	<queryString language="SQL">
		<![CDATA[SELECT V.order_no, V.order_date, V.amount, V.q10, V.price, V.notes,
	IFNULL(C.name, IFNULL(S.name, IFNULL(I.name, U.name))) AS object_name
FROM gold_transfer_balance V
	LEFT OUTER JOIN gold_customers C ON V.object_id = C.id AND V.object_type = 0
	LEFT OUTER JOIN gold_suppliers S ON V.object_id = S.id AND V.object_type = 1
	LEFT OUTER JOIN gold_investors I ON V.object_id = I.id AND V.object_type = 2
	LEFT OUTER JOIN cms_users U ON V.object_id = U.id AND V.object_type = 3
WHERE V.id =  $P{id}]]>
	</queryString>

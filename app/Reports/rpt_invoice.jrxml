	<subDataset name="dsTotalPay" uuid="989ad217-1c8d-42d6-96a6-daf6340d0759">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="jewelry"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="260"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="733"/>
		<parameter name="order_id" class="java.lang.Integer"/>
		<queryString language="SQL">
			<![CDATA[SELECT COUNT(SOP.id) AS no, SUM(SOP.total_weight - SOP.abate_weight) AS total_weight, SUM(SOP.gem_weight) AS gem_weight,
	SUM(SOP.abate_weight) AS abate_weight, SUM(SOP.gold_weight) AS gold_weight, SUM(SOP.fee) AS fee, 
	(IFNULL(SUM(SOP.amount), 0) + SO.pay_amount - SO.return_amount - SO.balance) AS amount
FROM gold_sale_orders SO 
	LEFT OUTER JOIN gold_sale_order_pays SOP ON SO.id = SOP.order_id
WHERE SOP.deleted_at IS NULL AND SO.id =  $P{order_id} 
GROUP BY SO.pay_amount, SO.return_amount, SO.balance]]>
		</queryString>

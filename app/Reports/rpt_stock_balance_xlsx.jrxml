		<queryString language="SQL">
			<![CDATA[SELECT I.bar_code, P.product_code, P.product_name, T.name AS product_type, 
	I.total_weight, I.gem_weight, I.gold_weight, I.qty, G.name AS group_name, I.q10
FROM gold_items I 
	LEFT OUTER JOIN gold_products P ON I.product_id = P.id
	LEFT OUTER JOIN gold_product_types T ON I.product_type_id = T.id
	LEFT OUTER JOIN gold_product_groups G ON I.product_group_id = G.id
WHERE I.deleted_at is null AND I.qty > 0 AND I.stock_id = $P{stock_id} 
ORDER BY T.name, G.name, I.id]]>
		</queryString>

		<queryString language="SQL">
			<![CDATA[SELECT P.id, P.order_no as pawn_order_no, P.order_date as pawn_order_date, 
	C.code as cus_code, C.name as cus_name, C.phone, 
    P.amount, P.interested_amount,
    P.due_date, P.min_days,
    CASE WHEN P.status = 1 THEN 1 ELSE null END as tk,
    CASE WHEN (P.id in (SELECT pawn_order_id FROM gold_pawn_oders_taking_details as D WHERE D.deleted_at is null AND D.order_id = $P{order_id})) THEN 1 ELSE null END as tt
FROM  gold_pawn_orders as P
	LEFT OUTER JOIN gold_customers as C ON P.customer_id = C.id

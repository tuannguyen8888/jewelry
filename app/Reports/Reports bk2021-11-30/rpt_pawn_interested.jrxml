	<queryString language="SQL">
		<![CDATA[SELECT SO.order_no, SO.order_date, C.name AS customer_name, C.address, C.phone, C.zalo_phone, 
	SO.due_date, SO.amount, SO.liquidation_notes,  B.name AS brand_name, U.name as user_name,
	CASE WHEN SO.liquidation_at = I.interested_date THEN SO.liquidation_method ELSE -1 END AS liquidation_method,
	I.interested_no, I.interested_date, I.last_interested_at, I.due_date AS days, I.amount AS interested_amount
FROM gold_pawn_orders SO
	INNER JOIN gold_pawn_order_interested I ON SO.id = I.order_id
	LEFT OUTER JOIN gold_customers C ON SO.customer_id = C.id
	LEFT OUTER JOIN gold_brands B ON SO.brand_id = B.id
	LEFT OUTER JOIN cms_users U ON SO.saler_id = U.id
WHERE I.id =  $P{id}]]>
	</queryString>

	<queryString language="SQL">
		<![CDATA[SELECT 
	SUM(A.begin_amount) as begin_amount, 
	SUM(A.begin_amount) + SUM(A.total_amount) as end_amount,
	SUM(A.total_order_count) as total_order_count,
	SUM(A.total_customer_count) as total_customer_count,
	SUM(A.total_amount) as total_amount,
	SUM(A.total_interested_amount) as total_interested_amount,
	SUM(A.total_finalization_amount) as total_finalization_amount,
	SUM(A.total_liquidation_count) as total_liquidation_count,
	SUM(A.total_liquidation_amount) as total_liquidation_amount,
	SUM(A.total_orver_count) as total_orver_count,
	SUM(A.total_orver_amount) as total_orver_amount
FROM (
	SELECT 
		SUM(PO.amount) as begin_amount,
		0 as total_order_count,
		0 as total_customer_count,
		0 as total_amount,
		0 as total_interested_amount,
		0 as total_finalization_amount,
		0 as total_liquidation_count,
		0 as total_liquidation_amount,
		0 as total_orver_count,
		0 as total_orver_amount
	FROM gold_pawn_orders as PO
	WHERE PO.deleted_at is null
		AND PO.status = 1
        AND PO.brand_id = $P{brand_id}
		AND PO.order_date < $P{from_date}
	UNION ALL
	SELECT 
		0 as begin_amount,
		COUNT(*) as total_order_count,
		COUNT(DISTINCT PO.customer_id) as total_customer_count,
		SUM(PO.amount) as total_amount,
		0 as total_interested_amount,
		0 as total_finalization_amount,
		0 as total_liquidation_count,
		0 as total_liquidation_amount,
		0 as total_orver_count,
		0 as total_orver_amount
	FROM gold_pawn_orders as PO
	WHERE PO.deleted_at is null
		AND PO.status = 1
        AND PO.brand_id = $P{brand_id}
		AND PO.order_date >= $P{from_date} AND PO.order_date < DATE_ADD($P{to_date}, INTERVAL 1 DAY)
	UNION ALL
	SELECT 
		0 as begin_amount,
		0 as total_order_count,
		0 as total_customer_count,
		0 as total_amount,
		SUM(CASE WHEN SO.liquidation_method = 0 AND SO.liquidation_at = POD.interested_date THEN POD.amount - SO.amount ELSE POD.amount END) as total_interested_amount,
		SUM(CASE WHEN SO.liquidation_method = 0 AND SO.liquidation_at = POD.interested_date THEN SO.amount ELSE 0 END) as total_finalization_amount,
		SUM(CASE WHEN SO.liquidation_method = 1 AND SO.liquidation_at = POD.interested_date THEN 1 ELSE 0 END) as total_liquidation_count,
		SUM(CASE WHEN SO.liquidation_method = 1 AND SO.liquidation_at = POD.interested_date THEN SO.amount ELSE 0 END) as total_liquidation_amount,
		0 as total_orver_count,
		0 as total_orver_amount
	FROM gold_pawn_orders SO
		INNER JOIN gold_pawn_order_interested POD ON SO.id = POD.order_id
		INNER JOIN gold_counters C ON POD.counter_id = C.id
	WHERE SO.deleted_at IS NULL AND POD.deleted_at IS NULL
		AND POD.interested_date >= $P{from_date} AND POD.interested_date < DATE_ADD($P{to_date}, INTERVAL 1 DAY)
		AND POD.brand_id = $P{brand_id} 		
	UNION ALL
	SELECT 
		0 as begin_amount,
		0 as total_order_count,
		0 as total_customer_count,
		0 as total_amount,
		0 as total_interested_amount,
		0 as total_finalization_amount,
		0 as total_liquidation_count,
		0 as total_liquidation_amount,
		COUNT(*) as total_orver_count,
		SUM(PO.amount) as total_orver_amount
	FROM gold_pawn_orders as PO
	WHERE PO.deleted_at is null
		AND PO.status = 1
        AND PO.brand_id = $P{brand_id}
		AND ((PO.last_interested_at is null and DATEDIFF(NOW(),PO.order_date) > 30) or (PO.last_interested_at is not null and  DATEDIFF(NOW(),PO.last_interested_at) > 30))
		AND PO.order_date >= $P{from_date} AND PO.order_date < DATE_ADD($P{to_date}, INTERVAL 1 DAY)
) as A]]>
	</queryString>
